	<plugin id="SequenceRetriever" name="Sequence Retriever" class="org.geworkbench.components.sequenceretriever.SequenceRetriever" source="sequenceretriever">
		<gui-area name="VisualArea"/>
	</plugin>

	<component-descriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Descriptor Schema.xsd">
	    <component class="org.geworkbench.components.sequenceRetriever.SequenceRetriever">
	        <online-help helpSet="org/geworkbench/components/sequenceRetriever/help/sequenceRetrieverHelpSet.hs"/>
