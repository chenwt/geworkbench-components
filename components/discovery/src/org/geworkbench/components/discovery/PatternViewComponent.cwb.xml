<?xml version="1.0" encoding="UTF-8"?>
<component-descriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="Descriptor Schema.xsd">
	<component name="Pattern Viewer"
		class="org.geworkbench.components.discovery.PatternViewComponent"
		version="1.0" 
		author="geWorkbench Development Team"
		authorURL="http://wiki.c2b2.columbia.edu/workbench/index.php/Home"
		tutorialURL="http://wiki.c2b2.columbia.edu/workbench/index.php/BLAST"
		toolURL="http://wiki.c2b2.columbia.edu/workbench/index.php/Home"
		description="The Pattern Discovery Results component displays
 		the results of Pattern Discovery Analysis.  Sequence hits can be returned to the Project
 		for further analysis."
		mustAccept="false" 
		category="visualizer" loadByDefault="true">
		<required-component
			class="org.geworkbench.components.discovery.PatternDiscoveryAnalysis" />
		<license><![CDATA[
			<html><head><meta http-equiv=Content-Type content="text/html; charset=windows-1252"></head>
				<body>----</body>
			</html>
		]]></license>
		
		<online-help helpSet="org/geworkbench/components/discovery/help/patternDiscoveryHelpSet.hs"/>

	</component>
	<plugin id="PatternViewComponent" name="Pattern Viewer"
		class="org.geworkbench.components.discovery.PatternViewComponent"
		source="discovery">
		<gui-area name="VisualArea" />
	</plugin>
</component-descriptor>
