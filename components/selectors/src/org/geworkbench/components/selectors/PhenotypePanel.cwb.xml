<component-descriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="Descriptor Schema.xsd">

	<component name="Arrays/Phenotypes"
		class="org.geworkbench.components.selectors.PhenotypePanel" version="1.0"
		author="geWorkbench Development Team" 
		authorURL="http://wiki.c2b2.columbia.edu/workbench/index.php/Home"
		toolURL="http://wiki.c2b2.columbia.edu/workbench/index.php/Home"
		tutorialURL="http://wiki.c2b2.columbia.edu/workbench/data/geWorkbench-HandsOnTraining.ppt"
		description="Displays a list of all microarrays in the current
 dataset.  Arrays can be assigned to phenotype/set labels and these sets
 can be manipulated."
		loadByDefault="true">
		<license><![CDATA[
			<html><head><meta http-equiv=Content-Type content="text/html; charset=windows-1252"></head>
				<body>----</body>
			</html>
		]]></license>

		<online-help
			helpSet="org/geworkbench/components/selectors/help/PanelsHelpSet.hs" />
		<menu-item path="Commands.Sets.Delete Set" mode="onFocus"
			var="Commands.Sets.Delete" />
		<menu-item path="Commands.Sets.Activate Set" mode="onFocus"
			var="Commands.Sets.Activate" />
		<menu-item path="Commands.Sets.Deactivate Set" mode="onFocus"
			var="Commands.Sets.Deactivate" />
		<!--  menu-item path="Commands.Sets.Rename Set" mode="onFocus"
			var="Commands.Sets.Rename" / -->
		<menu-item path="Commands.Sets.Add to Set" mode="onFocus"
			var="Commands.Sets.Add to Set" />
		<menu-item path="View.Clear Selection" mode="onFocus"
			var="View.Clear Selection" />

	</component>


	<plugin id="PhenotypePanel" name="Arrays/Phenotypes"
		class="org.geworkbench.components.selectors.PhenotypePanel" source="selectors">
		<gui-area name="SelectionArea" />
	</plugin>

</component-descriptor>