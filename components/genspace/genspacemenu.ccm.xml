			<html><head><meta http-equiv=Content-Type content="text/html; charset=windows-1252"></head>
				<body>----</body>
			</html>
		]]></license>
	</component>

	<plugin id="GenSpaceMenu" name="GenSpaceMenu"
		class="org.geworkbench.components.genspace.GenSpaceMenu" source="genspace" />

	<component-descriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="Descriptor Schema.xsd">
		<component class="org.geworkbench.engine.config.GenSpaceMenu">
			<menu-item path="Tools.genSpace" mode="always" var="Tools.genSpace" />
			<online-help helpSet="org/geworkbench/components/genspace/help/genspaceHelpSet.hs"/>
		</component>
	</component-descriptor>

