<component-descriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="Descriptor Schema.xsd">

	<component name="Cytoscape"
		class="org.geworkbench.components.cytoscape.CytoscapeWidget"
		version="1.0" author="C2B2"
		authorURL="http://wiki.c2b2.columbia.edu/workbench/index.php/Home"
		toolURL="http://wiki.c2b2.columbia.edu/workbench/index.php/Home"
		tutorialURL="http://wiki.c2b2.columbia.edu/workbench/index.php/Tutorial_-_Cytoscape_Network_Viewer"
		description="The Cytoscape ccmponent (http://cytoscape.org/)
 is used to display network graphs, as created e.g. by ARACNe.
 The component allows selected network nodes (genes) to be
 returned to a geWorkbench project for further analysis."
		visualizer="true" loadByDefault="true">

		<!--
			TODO Add this back in when Geneways becomes a released component.
			Currently, the geneways component has loadByDefault=true and
			hidden=true in genewayswidget.ccm.xml, so it is always loaded.
			<required-component
			class="org.geworkbench.components.cutenet.GenewaysWidget" />
		-->

		<license><![CDATA[
			<html><head><meta http-equiv=Content-Type content="text/html; charset=windows-1252"></head>
				<body>----</body>
			</html>
		]]></license>

		<online-help
			helpSet="org/geworkbench/components/cytoscape/help/cytoscapeHelpSet.hs" />
	</component>

	<plugin id="CytoscapeWidget" name="Cytoscape"
		class="org.geworkbench.components.cytoscape.CytoscapeWidget"
		source="cytoscape">
		<gui-area name="VisualArea" />
	</plugin>

</component-descriptor>
