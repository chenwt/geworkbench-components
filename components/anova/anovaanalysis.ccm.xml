<geaw-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ConfigurationFile Schema.xsd">

	<component  name="AnovaAnalysis"
							class="org.geworkbench.components.anova.AnovaAnalysis"
							version="1.0"
							author="MEV (TIGR)"
							authorURL="http://wiki.c2b2.columbia.edu/workbench/index.php/Plugins"
							toolURL="http://wiki.c2b2.columbia.edu/workbench/index.php/Plugins"
							tutorialURL="http://wiki.c2b2.columbia.edu/workbench/index.php/Tutorial_-_ANOVA"
							description="Analysis of Variance is used in geWorkbench to deterimine
 if significant differences in expression exist among three or more groups
 of microarray gene expression profiles.  Formally, it tests the null
 hypothesis that, for each gene, the mean expression of each group is equal."
							analysis="true"
							loadByDefault="true"
	 >
		<required-component class="org.geworkbench.components.anova.gui.TabularDataViewer" />
		<required-component class="org.geworkbench.components.colormosaic.ColorMosaicPanel" />
		<license><![CDATA[
			<html><head><meta http-equiv=Content-Type content="text/html; charset=windows-1252"></head>
				<body>----</body>
			</html>
		]]></license>

	</component>

	<plugin id="AnovaAnalysis" name="Anova Analysis" class="org.geworkbench.components.anova.AnovaAnalysis" source="anova"/>

	<component-descriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Descriptor Schema.xsd">
	    <component class="org.geworkbench.components.anova.AnovaAnalysis">
	        <online-help helpSet="org/geworkbench/components/anova/help/anovaHelpSet.hs"/>
	    </component>
	</component-descriptor>
	
</geaw-config>